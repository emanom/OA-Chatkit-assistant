{
  "name": "chatkit-node-backend-sdk",
  "version": "1.1.0",
  "description": "Node.js/TypeScript SDK for ChatKit custom backend protocol",
  "type": "module",
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "prepublishOnly": "npm run build && npm run type-check",
    "docs:api": "typedoc",
    "docs:serve": "npm run docs:api && mkdocs serve --dev-addr 0.0.0.0:8001",
    "docs:build": "npm run docs:api && mkdocs build"
  },
  "keywords": [
    "chatkit",
    "openai",
    "chat",
    "ai",
    "backend",
    "sdk"
  ],
  "author": "EvoTechMike",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/EvoTechMike/chatkit-node-backend-sdk.git"
  },
  "bugs": {
    "url": "https://github.com/EvoTechMike/chatkit-node-backend-sdk/issues"
  },
  "homepage": "https://github.com/EvoTechMike/chatkit-node-backend-sdk#readme",
  "engines": {
    "node": ">=18.0.0"
  },
  "devDependencies": {
    "@types/node": "^20.10.0",
    "@typescript-eslint/eslint-plugin": "^6.13.0",
    "@typescript-eslint/parser": "^6.13.0",
    "@vitest/coverage-v8": "^1.0.4",
    "eslint": "^8.54.0",
    "prettier": "^3.1.0",
    "tsup": "^8.0.1",
    "typedoc": "^0.28.13",
    "typedoc-plugin-markdown": "^4.9.0",
    "typescript": "^5.3.2",
    "vitest": "^1.0.4"
  },
  "dependencies": {
    "@openai/agents": "^0.1.9",
    "zod": "^3.25.76"
  }
}
